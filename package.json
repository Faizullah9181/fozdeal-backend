{
    "name": "fozdeal-backend-service",
    "version": "1.0.0",
    "description": "fozdeal backend service",
    "main": "dist/server.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/tvvignesh/node-skeleton.git"
    },
    "scripts": {
        "build": "copyfiles -u 1 \"./src/**/*.{css,html,ico,png,svg}\" dist/ && npx tsc && npx copyfiles -u 1 -e \"src/**/*.ts\" \"src/**/*.*\" dist/",
        "watch": "tsc -w -p ./",
        "debug": "npx cross-env NODE_ENV=staging nodemon --nolazy ./dist/server.js",
        "docker-debug": "npm run build && docker-compose up",
        "lint": "npx eslint --ext=.ts .",
        "test": "cross-env npm run build  && rm -rf database.sqlite3 && NODE_ENV=test npx sequelize-cli db:migrate && NODE_ENV=test npx sequelize-cli db:seed:all && NODE_ENV=test mocha --timeout 10000 -r dotenv/config ts-node/register dist/tests/**/*.test.js --exit",
        "document": "npx typedoc",
        "format": "prettier --write \"**/*.{ts,js,css,md}\"",
        "server": "node ./dist/server.js",
        "consumer": "node ./dist/consumer.js",
        "ts-consumer": "ts-node ./src/consumer.ts",
        "start-dev": "npm run format && npm run lint --fix && npx cross-env NODE_ENV=staging nodemon ./dist/server.js",
        "start-consumer": "npm run format && npm run lint --fix && npx cross-env NODE_ENV=staging nodemon ./dist/consumer.js",
        "start-alpha": "npm run build && npm run lint && npx cross-env NODE_ENV=alpha node ./dist/server.js",
        "start-secure": "npm run build && npm run lint && npx cross-env NODE_ENV=secure node ./dist/server.js",
        "coverage": "nyc --reporter=lcov --reporter=text-lcov npm run test",
        "sonar": "node sonar-project.js",
        "dev": "npm run build && npm run start"
    },
    "author": "Orca",
    "license": "MIT",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.465.0",
        "@aws-sdk/s3-request-presigner": "^3.465.0",
        "@orca/base-packages": "https://github.com/orca-dev-projects/base-packages",
        "@sendgrid/mail": "^7.7.0",
        "aws-sdk": "^2.1277.0",
        "axios": "^0.21.1",
        "bcrypt": "^5.1.0",
        "body-parser": "^1.19.0",
        "chai": "^4.3.4",
        "cors": "^2.8.5",
        "cron": "^2.0.0",
        "cross-env": "^7.0.3",
        "crypto": "^1.0.1",
        "crypto-js": "^4.1.1",
        "csvtojson": "^2.0.10",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-status-monitor": "^1.3.3",
        "glob": "^7.1.6",
        "helmet": "^4.4.1",
        "i18": "0.0.6",
        "jaeger-client": "^3.18.1",
        "jest": "^28.1.0",
        "jsonwebtoken": "^8.5.1",
        "method-override": "^3.0.0",
        "mocha": "^10.0.0",
        "moment": "^2.29.3",
        "multer": "^1.4.5-lts.1",
        "mustache-express": "^1.3.0",
        "mysql2": "^2.3.3",
        "nyc": "^15.1.0",
        "opentracing": "^0.14.5",
        "os": "^0.1.1",
        "passport": "^0.6.0",
        "pg": "^8.11.3",
        "pidusage": "^2.0.21",
        "prom-client": "^14.0.1",
        "razorpay": "^2.8.4",
        "reflect-metadata": "^0.1.13",
        "response-time": "^2.3.2",
        "sequelize": "^6.19.0",
        "sequelize-typescript": "^2.1.3",
        "sharp": "^0.31.3",
        "sonarqube-scanner": "^2.8.0",
        "sqlite3": "^5.0.8",
        "supertest": "^6.1.3",
        "swagger-ui-express": "^4.1.6",
        "ts-node": "^9.1.1",
        "underscore": "^1.13.3",
        "upc-generator": "^0.0.6",
        "uuid": "^9.0.1",
        "winston": "^3.3.3",
        "xss-clean": "^0.1.1",
        "yamljs": "^0.3.0"
    },
    "ava": {
        "files": [
            "dist/tests/*.ts"
        ],
        "extensions": [
            "ts"
        ],
        "failWithoutAssertions": false
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/body-parser": "^1.19.0",
        "@types/chai": "^4.2.16",
        "@types/express": "^4.17.11",
        "@types/express-serve-static-core": "^4.17.19",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/mocha": "^8.2.2",
        "@types/node": "^14.14.37",
        "@typescript-eslint/eslint-plugin": "^4.20.0",
        "@typescript-eslint/parser": "^4.20.0",
        "acorn": "^8.1.0",
        "ava": "^5.1.0",
        "copyfiles": "^2.4.1",
        "eslint": "^7.23.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "morgan": "^1.10.0",
        "nodemon": "^2.0.7",
        "prettier": "^2.2.1",
        "typedoc": "^0.22.17",
        "typescript": "^4.4.4"
    }
}
